 <script type="text/javascript">		var auto_news = new Array();function show_news(p,np)
{
	p = parseInt(p); //当前显示页码
	np = parseInt(np); //每页显示条数
	if (p<=0) p=1;
	if (np<=0) np=50;
	var nt = auto_news.length; //总条数
	var pt = Math.ceil(nt/np); //总页数
	if (p>pt) p=pt;
	var ns = (p-1)*np; //开始索引
	var ne = (p)*np; //结束索引(不包括)
	if (ne > nt) ne = nt;
	var __s = '<p align="left">&nbsp;</p>';
	for (var i=ns; i<ne; i++)
	{
		if (i%6 == 0)
		{
			__s += '<div style="padding:0 0 0 25px; text-align:left; " class="linkBlack01">';
		}
		if (i%6 == 0 && i%30 !=0)  {
			__s += '<div style="height:1px; overflow:hidden; border-bottom:1px #333 dotted; clear:both;"></div><p>&nbsp;</p>';
		}
		__s += '<li><a href="'+auto_news[i][1]+'">'+auto_news[i][0]+'</a> <span>('+auto_news[i][2]+')</span></li>';
		if (i%6 == 5)
		{
			__s += '</div>';
		}
	}
	__p = '';
	if (p>1)
	{
		__p += '<span><a href="javascript:show_news('+(p-1)+','+np+');void 0;">&lt;&lt;</a></span>&nbsp;';
	}
	else
	{
		__p += '<span>&lt;&lt;</span>&nbsp;';

	}
	for (i=1; i<=pt; i++)
	{
		if (i == p)
		{
			__p += "<span class='left-selected'>"+i+"</span>&nbsp;";
		}
		else
		{
			__p += "<span><a href='javascript:show_news("+i+","+np+");void 0'>"+i+"</a></span>&nbsp;";
		}
        if(i%20==0) __p += "<br/><br/>";
	}
	if (p<pt)
	{
		__p += '<span><a href="javascript:show_news('+(p+1)+','+np+');void 0;">&gt;&gt;</a><span>&nbsp;';
	}
	else
	{
		__p += '<span>&gt;&gt;</span>&nbsp;';
	}
    __p += '&nbsp;&nbsp; 共'+nt+'条,'+pt+'页';
	document.getElementById("news_content").innerHTML = __s;
	document.getElementById("news_page").innerHTML = __p;
}
show_news(1,30);
            </script>

