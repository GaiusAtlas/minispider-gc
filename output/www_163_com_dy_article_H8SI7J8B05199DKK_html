      jQuery.get('https://api.money.126.net/data/feed/0000001,1399001,money.api', {}, function(data) {
        if (!data) {
          return ;
        }
        var sh = data['0000001']
        var sz = data['1399001']
        if (sh && sz) {
          var _html = getHtml(sh) + getHtml(sz)
          jQuery('#post_side_mod_money').html(_html);
        } else {
          jQuery('#post_side_mod_money').hide();
        }
        function getHtml(obj) {
          var clazz = obj.code === '0000001' ? 'post_side_mod_money_sh' : 'post_side_mod_money_sz'
          var name = obj.code === '0000001' ? '上证' : '深成'
          var flag = 'up'
          if (obj.updown < 0) {flag = 'down'}
          return '<a href="http://quotes.money.163.com/stock#f=post2020_dy" class="'+clazz+' '+flag+'"><div class="post_side_mod_money_nums"><div class="price">'+obj.price.toFixed(2)+'</div><div class="updown">'+obj.updown+'</div><div class="percent">'+(obj.percent*100).toFixed(2)+'%</div></div><div class="post_side_mod_money_icon '+flag+'">'+name+'</div></a>'
        }
      }, 'jsonp');
    </script>
