                <script type="text/javascript">
                new DivSelect('slt_01');
                //搜索
                document.cheadSearchForm.onsubmit = function() {
                    var kw = this.q.value;
                    if (!kw || kw == '请输入关键词') {
                        alert('请输入搜索关键词!');
                        this.q.focus();
                        return false;
                    }
                    if (this.c.value == 'video') {
                        window.open('//search.sina.com.cn/?c=video&range=title&q=' + kw);
                        return false;
                    }
                }
                </script>
