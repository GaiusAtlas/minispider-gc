                (function() {
                    // 表单
                    var frm = document.cheadSearchForm;
                    // 下拉选择
                    var select = frm.c;
                    // 输入框
                    var input = frm.q;
                    // 提交按钮
                    var submit = function() {
                        frm.submit();
                    };
                    // 是否新闻
                    var isNews = function() {
                        return select.value === 'news';
                    };
                    // 提交
                    new searchsUggest({
                        input: input,
                        maxLen: 10,
                        placeholderStr: '请输入关键词',
                        showHotList: isNews,
                        showSuggestList: isNews,
                        onSelect: submit
                    });
                })();
