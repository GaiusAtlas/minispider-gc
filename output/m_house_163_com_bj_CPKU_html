<script type="text/javascript">
  // 检测是否是在网易新闻客户端下运行的
  function isNetEaseApp() {
    //return true;
    var ua = window.navigator.userAgent;
    if(/netease_news/gi.test(ua) || /NewsApp/gi.test(ua)){
      return true;
    }
    return false;
  }
  // wzw
  // document.getElementById("an_num").innerHTML = M_LP_INIT_DATA.lpTel + (M_LP_INIT_DATA.fenjiTel ? "转" + M_LP_INIT_DATA.fenjiTel : "");
  // document.getElementById("an_call_btn").href ="tel:" + M_LP_INIT_DATA.lpTel;
  if(isIndexDetail){
    if( isIphoneX() ){
      document.getElementById("float").setAttribute("class", "float hasShare footphotobox forx");
      document.getElementById("endFooter").setAttribute("class", "forx");

    }else{
      document.getElementById("float").setAttribute("class", "float hasShare footphotobox");
    }

  }
  if(M_LP_INIT_DATA && M_LP_INIT_DATA.city && M_LP_INIT_DATA.city === 'gz') {
    var className = document.getElementById("float").getAttribute("class")
    //document.getElementById("float").setAttribute("class", className + " gw");
  }
  if(document.getElementById("endShare")) {
    document.getElementById("endShare").onclick = function(){
      if(isNetEaseApp()){
        window.location.href = "share://";
      }else{
        document.getElementById("share").style.display = 'block';
      }
    }
  }
  
</script>
<script>
// 检测是否是在微信端下运行的
function isWeiXin(){
    var ua = window.navigator.userAgent.toLowerCase();
    if(ua.match(/MicroMessenger/i) == 'micromessenger'){
        return true;
    }else{
        return false;
    }
}



function initMini (callback) {
    //alert(0)
    $.ajax({
        url: 'https://game.house.163.com/web/weixin/wx1f343894d9cce45d/init',
        type: 'GET',
        dataType: 'jsonp',
        data: {
            redirect_uri: encodeURIComponent(location.href)
        },
        success: function(ret) {
            //alert(1)
            console.log(ret);
            wx.config({
                debug: false, // 开启调试模式,调用的所有api的返回值会在客户端alert出来，若要查看传入的参数，可以在pc端打开，参数信息会通过log打出，仅在pc端时才会打印
                appId: 'wx1f343894d9cce45d', // 必填，公众号的唯一标识
                timestamp: ret.timestamp, // 必填，生成签名的时间戳
                nonceStr: ret.nonceStr, // 必填，生成签名的随机串
                signature: ret.signature,// 必填，签名
                jsApiList: ['onMenuShareTimeline', 'onMenuShareAppMessage','updateAppMessageShareData','updateTimelineShareData'], // 必填，需要使用的JS接口列表
                openTagList: ['wx-open-launch-app', 'wx-open-launch-weapp'] // 可选，需要使用的开放标签列表，例如['wx-open-launch-app']
            });
            wx.ready(function () {
                //alert(2)
                var btn = document.getElementById('launch-btn');
                document.getElementById("gwBtn").style.display = "block"
                callback && callback()
                
                //alert(3)
            });
        }
    });
}
</script>
