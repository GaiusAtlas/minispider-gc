<script type="text/javascript">
  (function(){
    //小程序这个模块不要
    if(isWeChatApplet()){
      document.getElementById("endFooter").getElementsByClassName("clearfix")[0].style.display = "none";
      return false;
    }
    var protocol = window.location.protocol
    var randomTime = (new Date).getTime()
        var callName = null,
            head = document.getElementsByTagName("head")[0],
            script = document.createElement("script")

        callName = 'jsonp' + randomTime
        script.src = protocol + '//xf.house.163.com/bj/api/citylist?callback=' + callName
        window[callName] = function(res){
            try{
                //delete window[callName]
            }
            catch(e) {
                console.log(e)
            }
            var ename = ''
            //res.errorMsg = errorMsg[res.resultcode]
            if(res) {
              for(var x in res) {
                for(var i = 0;i < res[x].length;i++) {
                  if(res[x][i].ename === M_LP_INIT_DATA.city) {
                    document.getElementById("footList").innerHTML = res[x][i].name + '房产'
                  }
                }
              }

            }
            else {
                document.getElementById("footList").innerHTML = '北京房产'
            }
        }
        head.appendChild(script)

        document.getElementById("footCity").href = '/' + M_LP_INIT_DATA.city + '/lpkmobile/index.html#/index?refresh=true'
        document.getElementById("footList").href = '/' + M_LP_INIT_DATA.city + '/lpkmobile/index.html#/index.html#/list'
        document.getElementById("curLp").href = '//m.house.163.com/' + M_LP_INIT_DATA.city + '/' + M_LP_INIT_DATA.productid + '.html'
        document.getElementById("footPcUrl").href = '//' + M_LP_INIT_DATA.city + '.house.163.com/';
        document.getElementById("curLp").innerHTML = M_LP_INIT_DATA.productname
  })()
</script>
