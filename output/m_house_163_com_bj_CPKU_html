<script type="text/javascript">
/*查看大图的图片放缩 首页户型缩放 loadImg.js*/
function imageOnload(img, iWidth, iHeight, isMinSize ) {
	var curClientHeight =  iHeight ? iHeight : document.documentElement.clientHeight - 170;//目前设备可允许展示最大高度
	var clientWidth = iWidth ? iWidth : 750;
	var standardRadio = (clientWidth / curClientHeight).toFixed(0); //屏幕宽高比
	var oriWidth = img.width, oriHeight = img.height; //图片真实尺寸
	var realRadio = (oriWidth / oriHeight).toFixed(2); //图片真实尺寸比例
	var realWidth = 0, realHeight = 0;
	var marginTop = 0, marginLeft = 0;
	if(typeof isMinSize === "undefined"){
		if(clientWidth < oriWidth || curClientHeight < oriHeight){ //有一边尺寸超过可展示尺寸
			if(realRadio > standardRadio){ //宽图
				realWidth = clientWidth;
				marginLeft = 0;
				realHeight = clientWidth * oriHeight / oriWidth;
				marginTop = (curClientHeight - realHeight) / 2;
			}else{
				realHeight = curClientHeight;
				marginTop = 0;
				realWidth = curClientHeight * oriWidth / oriHeight;
				marginLeft = (clientWidth - realWidth) / 2;
			}
		}else{
			realWidth = oriWidth;
			realHeight = oriHeight;
			marginLeft = (clientWidth - oriWidth) / 2;
			marginTop = (curClientHeight - oriHeight) / 2;
		}
	}else{
		if(isMinSize === 1){ //定宽展示
			realWidth = clientWidth;
			realHeight = clientWidth * oriHeight / oriWidth;
			marginTop = curClientHeight - realHeight > 0 ? (curClientHeight - realHeight) / 2 :- (realHeight - curClientHeight) / 2;
		}
	}
	img.style.marginTop = marginTop + "px";
	img.style.marginLeft = marginLeft + "px";
	img.style.width = realWidth + "px";
	img.style.height = realHeight + "px";
}


/*户型图的图片放缩*/
function imageHuxingOnload(img) {
	//console.log(img);
	var clientWidth = 670;
	var curClientHeight2 = 892;
	var standardRadio = clientWidth / curClientHeight2; //屏幕宽高比
	var oriWidth = img.width, oriHeight = img.height; //图片真实尺寸
	var realRadio = img.width / oriHeight; //图片真实尺寸比例
	var realWidth = 0, realHeight = 0;
	var marginTop = 0, marginLeft = 0;

	if(clientWidth < oriWidth || curClientHeight2 < oriHeight){ //有一边尺寸超过可展示尺寸
		if(realRadio > standardRadio){ //宽图
			realWidth = clientWidth;
			marginLeft = 0;
			realHeight = clientWidth * oriHeight / oriWidth;
			marginTop = (curClientHeight2 - realHeight) / 2;
		}else{
			realHeight = curClientHeight2;
			marginTop = 0;
			realWidth = curClientHeight2 * oriWidth / oriHeight;
			marginLeft = (clientWidth - realWidth) / 2;
		}
	}else{
		realWidth = oriWidth;
		realHeight = oriHeight;
		marginLeft = (clientWidth - oriWidth) / 2;
		marginTop = (curClientHeight2 - oriHeight) / 2;
	}
	img.style.marginTop = marginTop + "px";
	img.style.marginLeft = marginLeft + "px";
	img.style.width = realWidth + "px";
	img.style.height = realHeight + "px";
}

</script>
