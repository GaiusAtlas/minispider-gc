<script type="text/javascript">
  var __newsapp_sharetext = document.getElementById('__newsapp_sharetext')
  var __newsapp_sharephotourl = document.getElementById('__newsapp_sharephotourl')
  var __newsapp_sharewxtext = document.getElementById('__newsapp_sharewxtext')
  var __newsapp_sharewxtitle = document.getElementById('__newsapp_sharewxtitle')
  var __newsapp_sharewxurl = document.getElementById('__newsapp_sharewxurl')
  var __newsapp_sharewxthumburl = document.getElementById('__newsapp_sharewxthumburl')
  var pageName = "", desc = "", url = location.href.replace(/\?.*/, "")
  if(url.indexOf("detail") > -1) {
    pageName = "楼盘详情"
    desc = "详细信息|售楼处电话|开发商详情"
  }
  else if(url.indexOf("news") > -1) {
    pageName = "楼盘快讯"
    desc = "最新活动|施工进度|优惠信息"
  }
  else if(url.indexOf("huxing") > -1) {
    pageName = "户型列表"
    desc = "房价|户型|开盘时间"
  }
  else if(url.indexOf("nearby") > -1) {
    pageName = "周边地图"
    desc = "周边环境|配套信息"
  }
  else if(url.indexOf("photo") > -1) {
    pageName = "相册列表"
    desc = "效果图|户型|实景图"
  }
  else if(url.indexOf("comment") > -1) {
    pageName = "用户点评"
    desc = "怎么样|评价|用户点评"
  }
  else {
    pageName = "楼盘首页"
    desc = "房价|户型|开盘时间"
  }
  __newsapp_sharetext.innerHTML = __newsapp_sharetext.innerHTML.replace(/楼盘名/g, M_LP_INIT_DATA.productname).replace(/城市名/g, M_LP_INIT_DATA.cityChina).replace(/当前页/g, pageName)
  __newsapp_sharephotourl.innerHTML = M_LP_INIT_DATA.topphoto
  __newsapp_sharewxtitle.innerHTML = __newsapp_sharetext.innerHTML  
  __newsapp_sharewxthumburl.innerHTML = __newsapp_sharephotourl.innerHTML
  __newsapp_sharewxtext.innerHTML = __newsapp_sharewxtext.innerHTML.replace(/楼盘名/g, M_LP_INIT_DATA.productname).replace(/城市名/g, M_LP_INIT_DATA.cityChina).replace(/描述/g, desc)
  __newsapp_sharewxurl.innerHTML = url
</script>
